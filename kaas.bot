#name kaas
#author Lieuwe Rooijakkers

#page 0
	page 15

#page 15
main:
	look res
	if res == 2 || res == 8, @inject
	if res == 0, @build
	goto @turn // goes to begin main after the turn.

build:
	build 2

	trans 0, 0
	trans 15, 15
	trans 14, 14
	shake-awake

	//goto _prevloc - 1
	goto @main

turn:
	rot 1
	goto @main

inject:
	sto i, 0
injectloop:
	look res
	trans 14, i
	sto i, i+1
	if i<16 && (res == 2 || res == 8), @injectloop

	goto @main

#page 14
	suicide
